<%= simple_form_for @supervisor, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  <%= error_span(@supervisor[:name]) %>
  <%= f.input :last_name %>
  <%= error_span(@supervisor[:last_name]) %>
  <%= f.input :app_user_id %>
  <%= error_span(@supervisor[:app_user_id]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                supervisors_path, :class => 'btn btn-default' %>
<% end %>
